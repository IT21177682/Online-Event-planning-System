<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
  version="3.1">
  
         
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.customer.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>eventInsert</servlet-name>
		<servlet-class>com.customer.Customerinsert</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>eventInsert</servlet-name>
		<url-pattern>/insert</url-pattern>
	</servlet-mapping>
   
   <servlet>
		<servlet-name>updateEvent</servlet-name>
		<servlet-class>com.customer.UpdateEventServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>updateEvent</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>deleteEvent</servlet-name>
		<servlet-class>com.customer.deleteEvenetServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>deleteEvent</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
	
</web-app>